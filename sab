local VirtualInputManager = game:GetService("VirtualInputManager")

-- Function to press keys 1-9
local function pressKeys()
    for i = 1, 9 do
        local keyCode = Enum.KeyCode["One"]
        if i == 2 then keyCode = Enum.KeyCode.Two
        elseif i == 3 then keyCode = Enum.KeyCode.Three
        elseif i == 4 then keyCode = Enum.KeyCode.Four
        elseif i == 5 then keyCode = Enum.KeyCode.Five
        elseif i == 6 then keyCode = Enum.KeyCode.Six
        elseif i == 7 then keyCode = Enum.KeyCode.Seven
        elseif i == 8 then keyCode = Enum.KeyCode.Eight
        elseif i == 9 then keyCode = Enum.KeyCode.Nine end

        -- Simulate key press
        VirtualInputManager:SendKeyEvent(true, keyCode, false, game)
        VirtualInputManager:SendKeyEvent(false, keyCode, false, game)
    end
end

-- Loop every 0.01s
while task.wait(0.01) do
    pressKeys()
end
